    "iota",
    "shimmer",
    "stardust",
    "tangle",
    "crypto"
  ],
  "homepage": "https://github.com/tanglePHP",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Stefan Braun",
      "homepage": "https://tanglephp.com",
      "role": "Developer"
    }
  ],
  "support": {
